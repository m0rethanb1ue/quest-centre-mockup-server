{
  "name": "quest-centre-mockup",
  "author": "morethanb1ue <khoadduy@gmail.com>",
  "version": "0.0.1",
  "description": "Quest Centre Mockup.",
  "type": "module",
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "^18",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.0",
    "@typescript-eslint/parser": "7.0",
    "copyfiles": "2.4.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2",
    "rimraf": "5.0.7",
    "ts-node": "10.9.2",
    "typescript": "5.4",
    "unique-names-generator": "4.7.1"
  },
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon --watch dist -e js src/index.js",
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc --write .",
    "dump:db": "yarn clean && yarn build && rm -rf src/db/json/*.json && node dist/src/db/index.js && mkdir ./dist/src/db/json && cp ./src/db/json/*.json ./dist/src/db/json/"
  },
  "dependencies": {
    "body-parser": "1.20.2",
    "chalk": "5.3.0",
    "express": "4.19.2",
    "lodash": "4.17.21",
    "lowdb": "7.0.1",
    "nodemon": "3.1.3",
    "tslib": "2.6",
    "uuid": "9.0.1"
  },
  "engines": {
    "node": ">=18.x.x"
  }
}
